    ingress.kubernetes.io/add-base-url: 'true'
    ingress.kubernetes.io/rewrite-target: /
spec:
  rules:
  - http:
      paths:
      - path: /$WERCKER_APPLICATION_OWNER_NAME/user99/
        backend:
          serviceName: rest-jscreditscore-svc99
          servicePort: 8099
